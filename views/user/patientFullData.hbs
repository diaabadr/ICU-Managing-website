<body>
    <div class="container">
      <div class="header">
        <div class="logo home-btn">
          <p class="home-btn">New Life</p>
          <h2 class="home-btn">ICU</h2>
        </div>
        <div class="links">
          <ul class="links">
            <li><a href="#">Department</a></li>
            <li><a href="#">Contact US</a></li>
            <li><a href="#">About US</a></li>
            <li><a href="#" class="log-out">Log Out</a></li>
          </ul>
        </div>
      </div>
    </div>
    <div class="data-report">
        <div class="patients doctor-patients messages">
            <div class="contains">
                <div class="sp-header patients-refresh complaints">Patient Data</div>
                <div class="pa-data row">
                    <div class="data-name data col-8 right">
                        <h3>Name</h3>
                        <h2>Abelrhman Yasser</h2>
                    </div>
                    <div class="data data-age col-2 right left">
                        <h3>Age</h3>
                        <h2>22</h2>
                    </div>
                    <div class="data data-gender col-2 left">
                        <h3>Gender</h3>
                        <h2>Male</h2>
                    </div>
                    <div class="data data-adress col-6 right">
                        <h3>Address</h3>
                        <h2>Shikh Zaid - 6 October</h2>
                    </div>
                    <div class="data data-phone col-6 left">
                        <h3>Phone</h3>
                        <h2>01211035528</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="data-report">
        <div class="patients doctor-patients messages">
            <div class="contains">
                <div class="sp-header patients-refresh complaints">Hospital Data</div>
                <div class="pa-data row">
                    <div class="data-name data col-5 right">
                        <h3>Department</h3>
                        <h2>Medical</h2>
                    </div>
                    <div class="data data-age col-2 right left">
                        <h3>Room No</h3>
                        <h2>2</h2>
                    </div>
                    <div class="data data-gender col-5 left">
                        <h3>Arrival Date</h3>
                        <h2>May 12 2022</h2>
                    </div>
                    <div class="data data-adress col-6 right">
                        <h3>Doctor Name</h3>
                        <h2>Abdelrhman Yasser</h2>
                    </div>
                    <div class="data data-phone col-6 left">
                        <h3>Nurse Name</h3>
                        <h2>Diaa Badr</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="data-report">
        <div class="patients doctor-patients messages">
            <div class="contains">
                <div class="sp-header patients-refresh complaints">Medical Data</div>
                <div class="pa-data row">
                    <div class="data-name data col-5 right">
                        <h3>Diagnosis</h3>
                        <h2>Mareed</h2>
                    </div>
                    <div class="data data-age col-5 right left">
                        <h3>Medicines</h3>
                        <h2>Cometrix</h2>
                    </div>
                    <div class="data data-gender col-2 left">
                        <h3>Blood Tybe</h3>
                        <h2>A+</h2>
                    </div>
                    <div class="data data-adress col-4 right">
                        <h3>Last Blood Presure</h3>
                        <h2>120</h2>
                    </div>
                    <div class="data data-phone col-4 left right">
                        <h3>Last Blood Glocuse</h3>
                        <h2>130</h2>
                    </div>
                    <div class="data data-phone col-4 left">
                        <h3>Progrss</h3>
                        <h2>78%</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
<script src="../JS/all.min.js"></script>
<script src="../JS/main.js"></script>
